@model IEnumerable<QuizMaster.Models.Examiner>

@{
    ViewData["Title"] = "CalculateScores";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body>


    <section>
        <p>Thank You for Thaking the Exam</p>
        <p>You Took To of @ViewBag.Total Question</p>
        <p>You got @ViewBag.Score questions correctly out of  @ViewBag.Total questions</p>
        <h1>You score is @ViewBag.Pass / @ViewBag.Total</h1>
    </section>


</body>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
